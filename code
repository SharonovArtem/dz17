using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Web;

namespace jun1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int N = random.Next(1, 28);
            int summand = N;
            int maxThreeDigitNumber = 999;

            for (int i = 0; i <= maxThreeDigitNumber; i += summand)
            {
                Console.WriteLine(i);
            }
        }
    }
}
