using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Web;

namespace jun1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int minNumber = 1;
            int maxNumber = 28;
            int minLimit = 100;
            int number = random.Next(minNumber, maxNumber);
            int numberCount = 0;
            int maxLimit = 999;

            for (int i = minLimit; i <= maxLimit && i >= minLimit; i += number)
            {
                if (i >= minLimit)
                {
                    numberCount += 1;
                }     
                Console.WriteLine(i);
            }
            Console.WriteLine(number);
            Console.WriteLine(numberCount);
        }
    }
}
